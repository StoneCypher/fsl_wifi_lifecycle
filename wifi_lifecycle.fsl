machine_language : en;
machine_version  : 1.0.0;
fsl_version      : 1.0.0;

start_states     : [Disconnected];





Disconnected 'scan'       =>
Scanning     'associate'  =>
Associating  'finalize'   =>
Associated   'complete'   =>
Completed    'connect'    =>
Connected    'disconnect' => Disconnected;

[Scanning Associating Associated Completed] 'fail' -> Disconnected;